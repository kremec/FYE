<Window x:Class="FYE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FYE"
        mc:Ignorable="d"
        Title="FYE" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal">
            <Button x:Name="ImportJSONButton" Content="Choose JSON data files" Click="ImportJSONButton_Click" Margin="10"/>
            <Button x:Name="ExportJSONButton" Content="Export better JSON" Click="ExportJSONButton_Click" Margin="10"/>
        </StackPanel>
    </StackPanel>
</Window>
